{
  "name": "types-storage",
  "private": false,
  "version": "2.0.1",
  "type": "module",
  "description": "Type-safe storage package built for TypeScript.",
  "main": "dist/types-storage.cjs",
  "module": "dist/types-storage.js",
  "exports": {
    ".": {
      "import": "./dist/types-storage.js",
      "require": "./dist/types-storage.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/svnrnns/types-storage.git"
  },
  "keywords": [
    "localstorage",
    "sessionstorage",
    "typescript",
    "type-safe",
    "storage"
  ],
  "author": "svnrnns",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:verbose": "vitest run --reporter=verbose",
    "build": "vite build",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "happy-dom": "^20",
    "typescript": "^5",
    "vite": "^7",
    "vite-plugin-dts": "^4",
    "vitest": "^4"
  },
  "peerDependencies": {
    "zod": "^3"
  },
  "dependencies": {
    "zod": "^3"
  }
}
